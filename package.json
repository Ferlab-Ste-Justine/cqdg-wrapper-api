{
  "name": "cqdg-wrapper-next",
  "version": "2.0.3",
  "description": ":factory: a data-model aware GraphQL API that sits above an Elasticsearch cluster",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": "^18.19.1",
    "npm": "^10.2.4"
  },
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --no-warnings ./dist/src/index.js",
    "dev": "tsx watch ./src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf node_modules dist package-lock.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "buildAndRun": "npm run build; npm start",
    "admin-project": "node --experimental-modules=node --es-module-specifier-resolution=node admin/run.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ferlab-Ste-Justine/cqdg-api-arranger.git"
  },
  "author": "atoulouse@ferlab.bio",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/Ferlab-Ste-Justine/cqdg-api-arranger/issues"
  },
  "homepage": "https://github.com/Ferlab-Ste-Justine/cqdg-api-arranger#readme",
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@awaitjs/express": "^0.8.0",
    "@ferlab/next": "^0.0.12",
    "@opensearch-project/opensearch": "^2.8.0",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "date-fns": "^3.6.0",
    "dateformat": "^5.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "filesize": "^3.6.1",
    "graphql": "16.8.1",
    "graphql-fields": "^2.0.3",
    "http-status-codes": "^2.1.4",
    "jsonpath": "^1.1.1",
    "keycloak-connect": "^24.0.4",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.5",
    "python-shell": "^3.0.0",
    "regenerator-runtime": "^0.13.9",
    "tar-stream": "^3.1.7",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.24",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.3.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.1.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
